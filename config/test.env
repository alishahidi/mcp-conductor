SPRING_PROFILES_ACTIVE=dev
SPRING_MAIN_WEB_APPLICATION_TYPE=none
SPRING_MAIN_BANNER_MODE=off
SPRING_AI_MCP_SERVER_TYPE=ASYNC
SPRING_AI_MCP_SERVER_TRANSPORT=STDIO

# SSH Configuration
SSH_DEFAULT_HOST=192.168.2.93
SSH_DEFAULT_PORT=22
SSH_DEFAULT_USERNAME=ali
SSH_PRIVATE_KEY_PATH=/home/ali/.ssh/mcp-conductor-test
SSH_PRIVATE_KEY_PASSPHRASE=

# Docker Configuration
DOCKER_HOST=unix:///var/run/docker.sock
DOCKER_API_VERSION=
DOCKER_TLS_VERIFY=false
DOCKER_CERT_PATH=
DOCKER_REGISTRY_URL=
DOCKER_REGISTRY_USERNAME=
DOCKER_REGISTRY_PASSWORD=

# Security Configuration
SECURITY_USER=admin
SECURITY_PASSWORD=test123
SECURITY_ENABLED=true
SECURITY_COMMAND_STRICT_MODE=false
SECURITY_COMMAND_ALLOWED=ls,ps,df,free,cat,echo,pwd,whoami,docker,git,curl,wget,systemctl,service,nginx,apt,yum,dnf
SECURITY_PATH_ALLOWED=/tmp,/var/log,/home,/opt,/usr/local
SECURITY_PATH_BLOCKED=/etc/passwd,/etc/shadow,/root

# Rate Limiting
RATE_LIMIT_CAPACITY=1000
RATE_LIMIT_REFILL_TOKENS=100
RATE_LIMIT_REFILL_DURATION_MINUTES=1

# Audit Configuration
AUDIT_ENABLED=true
AUDIT_LOG_FILE=logs/audit.log
AUDIT_LOG_ENABLED=true

# Async Configuration
ASYNC_EXECUTOR_CORE_POOL_SIZE=10
ASYNC_EXECUTOR_MAX_POOL_SIZE=50
ASYNC_EXECUTOR_QUEUE_CAPACITY=500
ASYNC_EXECUTOR_THREAD_NAME_PREFIX=mcp-async-

# MCP Server Configuration
SPRING_AI_MCP_SERVER_NAME=mcp-conductor
SPRING_AI_MCP_SERVER_VERSION=1.0.0

# Cache Configuration
SPRING_CACHE_TYPE=simple

# Spring Cloud Configuration
SPRING_CLOUD_COMPATIBILITY_VERIFIER_ENABLED=false
